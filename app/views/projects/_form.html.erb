<table summary="Project form fields">
	<tr>
		<th align="left">Name:</th>
		<td><%= f.text_field(:name) %></td>
	</tr>
	<tr>
		<th align="left">Project Number:</th>
		<td><%= f.text_field(:project_number) %></td>
	</tr>
	<tr>
		<th align="left">Address:</th>
		<td><%= f.text_field(:address) %></td>
	</tr>
